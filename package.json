    "karma-script-launcher": "~0.1.0",
    "karma-systemjs": "^0.7.2",
    "rxjs": "5.0.0-beta.12",
    "shelljs": "^0.7.0",
    "shx": "^0.1.4",
    "systemjs": "^0.18.4",
    "tslint": "=2.5.0",
    "typedoc": "~0.5.0",
    "typedoc-plugin-external-module-name": "^1.0.2",
